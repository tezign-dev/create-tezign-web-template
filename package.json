{
  "name": "tezign-web-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config=webpack/debug.js",
    "build": "npm run clean && NODE_ENV=production node_modules/.bin/webpack --config=webpack/build.js",
    "build:test": "__ENV__=TEST npm run build",
    "build:prod": "__ENV__=PROD npm run build",
    "clean": "rm -f -r -d dist"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "commons.js": "^1.3.26",
    "history": "^4.7.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "tezign-ui": "^2.17.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.133",
    "@types/node": "^11.13.7",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-router-dom": "^4.3.2",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "css-loader": "^2.1.1",
    "eslint": "^7.20.0",
    "eslint-config-alloy": "^3.10.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.1",
    "husky": "^4.3.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.8.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^4.1.5",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "tezign-ui"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "ie >= 10",
    "last 2 versions"
  ],
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --format codeframe --fix"
    ],
    "*.{css,less,sass,scss}": [
      "stylelint --fix"
    ]
  },
  "author": "tezign",
  "license": ""
}
